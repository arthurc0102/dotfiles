        "rules": [
          {
            "description": "Swap option and cmd in terminal and vscode",
            "manipulators": [
              {
                "type": "basic",
                "conditions": [
                  {
                    "type": "frontmost_application_if",
                    "bundle_identifiers": [
                      "^com\\.apple\\.Terminal$",
                      "^com\\.googlecode\\.iterm2$",
                      "^com\\.microsoft\\.VSCode$",
                      "^com\\.microsoft\\.VSCodeInsiders$"
                    ]
                  }
                ],
                "from": {
                  "key_code": "left_command",
                  "modifiers": {
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "left_option"
                  }
                ]
              },
              {
                "type": "basic",
                "conditions": [
                  {
                    "type": "frontmost_application_if",
                    "bundle_identifiers": [
                      "^com\\.apple\\.Terminal$",
                      "^com\\.googlecode\\.iterm2$",
                      "^com\\.microsoft\\.VSCode$",
                      "^com\\.microsoft\\.VSCodeInsiders$"
                    ]
                  }
                ],
                "from": {
                  "key_code": "left_option",
                  "modifiers": {
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "left_command"
                  }
                ]
              }
            ]
          },
          {
